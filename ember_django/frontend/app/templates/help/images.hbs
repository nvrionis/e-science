<p></p>
<div class="well well-lg text-center v-center">
	<div class="row">
		<h2 id = 'id_title_help_images'>Images Help</h2>
	</div>
</div>
	<ul id="myTab" class="nav nav-tabs">
		{{#each image in images}}
			{{#if image.active_image}}
			<li class="active">
				<a  {{bind-attr href="image.image_href"}} data-toggle="tab">{{image.image_name}}</a>
			</li>
			{{else}}
			<li>
				<a {{bind-attr href="image.image_href"}} data-toggle="tab">{{image.image_name}}</a>
			</li>
			{{/if}}
		{{/each}}
	</ul>
<div id="TabContent" class="tab-content">
	{{#each image in images}}
		{{#if image.active_image}}
		<div {{bind-attr id="image.image_pithos_uuid"}} class="tab-pane fade in active">
			<div class="row">
			<table class="table table-striped table-hover table-border">
				<tr>
					<td class="col-sm-6 text-center"><em><b>Name</b></em></td>
					<td class="col-sm-6 text-center"><em><b>Version</b></em></td>
				</tr>
				{{#each pair in image.components}}
				<tr>
					{{#each component in pair}}
					<td class="col-sm-6 text-center"><b>{{component}}</b></td>
					{{/each}}
				</tr>
				{{/each}}
			</table>
			</div>
		</div>
		{{else}}
		<div {{bind-attr  id="image.image_pithos_uuid"}} class="tab-pane fade">
			<table class="table table-striped table-hover table-border">
				<tr>
					<td class="col-sm-6 text-center"><em><b>Name</b></em></td>
					<td class="col-sm-6 text-center"><em><b>Version</b></em></td>
				</tr>
				{{#each pair in image.components}}
				<tr>
					{{#each component in pair}}
					<td class="col-sm-6 text-center"><b>{{component}}</b></td>
					{{/each}}
				</tr>
				{{/each}}
			</table>
		</div>
		{{/if}}
	{{/each}}
	</div>
</div>